#include <stdio.h>
#include <stdlib.h>

int main()
{
//script that prints name and gpa
char name[50];
printf("Please enter your name : ");
scanf("%s", &name);

double gpa;
printf("Please enter your gpa : ");
scanf("%lf", &gpa);

printf("Your name is %s and your gpa is %lf", name, gpa);
return 0;
}

